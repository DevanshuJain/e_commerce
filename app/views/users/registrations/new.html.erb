<h2>Sign up</h2>

  <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>



  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name, autofocus: true, autocomplete: "name" %>
  </div>

  <div class="field">
    <%= f.label :mobile %><br />
    <%= f.text_field :mobile, autofocus: true, autocomplete: "mobile" %>
  </div>


  <div class="field">
    <%= f.fields_for :addresses, Address.new do |address_form|%>
      <div>
        <%= address_form.label :adress %><br />
        <%= address_form.text_field :adress, autofocus: true, autocomplete: "adress" %>
      </div>
     
      <div class="field">
        <%= address_form.label :city %><br />
        <%= address_form.text_field :city, autofocus: true, autocomplete: "city" %>
      </div>

      <div class="field">
        <%= address_form.label :State %><br />
        <%= address_form.text_field :state, autofocus: true, autocomplete: "state" %>
      </div>
     
      <div class="field">
        <%= address_form.label :Pin %><br />
        <%= address_form.text_field :pin, autofocus: true, autocomplete: "pin" %>
      </div>
    <% end %>
  </div>

  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field">
    <%= f.label :password %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "off" %>
  </div>

  <div class="field">
    <%= f.label :password_confirmation %><br />
    <%= f.password_field :password_confirmation, autocomplete: "off" %>
  </div>

  <div class="actions">
    <%= f.submit "Sign up" %>
  </div>
<% end %>


<p class="navbar-text pull-right">
    <% if seller_signed_in? %>
      <%= link_to 'seller_Edit profile', edit_seller_registration_path, :class => 'navbar-link' %> |
      <%= link_to "seller_Logout", destroy_seller_session_path, method: :delete, :class => 'navbar-link'  %>
    <% else %>
      <%= link_to "seller_Sign up", new_seller_registration_path, :class => 'navbar-link'  %> |
      <%= link_to "seller_Login", new_seller_session_path, :class => 'navbar-link'  %>
    <% end %>
  </p></br>


<%= render "users/shared/links" %>
